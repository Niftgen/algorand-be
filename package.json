{
  "name": "be",
  "version": "1.0.0",
  "scripts": {
    "pm2": "tsc && pm2 start processes.json --no-daemon --env=development",
    "watch": "nodemon start",
    "build": "tsc",
    "start": "tsc && node ./bin/www",
    "db:generate": "knex migrate:make -x ts --cwd cdk/src/lambda/db --env=development",
    "db:migrate": "knex migrate:latest --cwd cdk/src/lambda/db --env=development",
    "db:rollback": "knex migrate:rollback --cwd cdk/src/lambda/db --env=development",
    "seed:make": "knex seed:make -x ts --cwd cdk/src/lambda/db --env=development",
    "seed:run": "knex seed:run --cwd cdk/src/lambda/db --env=development"
  },
  "repository": "git@github.com:Niftgen-Algorand/be.git",
  "author": "Nitfgen",
  "license": "MIT",
  "dependencies": {
    "@aws-cdk/aws-appsync-alpha": "2.44.0-alpha.0",
    "@aws-sdk/client-pinpoint": "3.183.0",
    "@graphql-tools/graphql-file-loader": "^7.5.5",
    "@graphql-tools/load": "^7.7.7",
    "@graphql-tools/schema": "^9.0.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.8.1",
    "@types/pg": "8.6.5",
    "ajv-formats": "2.1.1",
    "algosdk": "^1.21.0",
    "axios": "^0.27.2",
    "constructs": "10.1.121",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "form-data": "4.0.0",
    "formidable": "2.0.1",
    "graphql": "^16.6.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-upload": "13.0.0",
    "jest": "^27.4.7",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "mixpanel": "0.17.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "objection": "^3.0.1",
    "pg": "^8.8.0",
    "pm2": "^5.2.0",
    "source-map-support": "0.5.16",
    "superagent": "^8.0.2",
    "supertest": "^6.3.0",
    "typescript": "4.6.4"
  },
  "devDependencies": {
    "ts-node": "^10.9.1"
  }
}
